#include "qca955x.dtsi"

#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>

/ {
	compatible = "meraki,mr18", "qca,qca9558";
	model = "Meraki MR18";

	chosen {
		bootargs = "console=ttyS0,115200n8 ubi.mtd=3,512";
	};

	aliases {
		led-boot = &led_power_white;
		led-failsafe = &led_power_orange;
		led-running = &led_power_white;
		led-upgrade = &led_power_orange;
	};

	keys {
		compatible = "gpio-keys";

		reset {
			label = "reset";
			linux,code = <KEY_RESTART>;
			gpios = <&gpio 17 GPIO_ACTIVE_LOW>;
		};
	};

	leds {
		compatible = "gpio-leds";

		led_power_white: power_white {
			label = "red:power";
			gpios = <&gpio 18 GPIO_ACTIVE_LOW>;
		};

		led_power_orange: power_orange {
			label = "orange:power";
			gpios = <&gpio 21 GPIO_ACTIVE_LOW>;
			default-state = "on";
		};
	};

	leds-nu801 {
		compatible = "numen,leds-nu801";

		tricolor {
			label = "tricolor";
			num_leds = <1>;
			cki = <11>;
			sdi = <12>;
			//lei = <1>;
			ndelay = <500>;
			init_brightness = <0 0 0>;
			default_trigger = "none";
			led_colors = "red green blue";
		};
	};
};

&nand {
	status = "okay";

	nand-ecc-mode = "soft";
    nand-ecc-algo = "bch";

		partitions {
		compatible = "fixed-partitions";
		#size-cells = <1>;

		partition@0 {
			label = "nandloader";
			reg = <0x0000000 0x0080000>;
			read-only;
		};

		partition@80000 {
			label = "kernel";
			reg = <0x0080000 0x0800000>;
		};

		partition@880000 {
			label = "recovery";
			reg = <0x0880000 0x0800000>;
		};

		partition@1080000 {
			label = "ubi";
			reg = <0x1080000 0x6f00000>;
		};

		partition@0x7fe0000 {
			label = "odm-caldata";
			reg = <0x7fe0000 0x0020000>;
		};
	};
};

&mdio0 {
	status = "okay";

	phy3: ethernet-phy@3 {
		reg = <3>;
		eee-broken-100tx;
		eee-broken-1000t;
	};
};

&eth0 {
	status = "okay";

	pll-data = <0xa6000000 0xa0000101 0x80001313>;

	phy-mode = "rgmii";
	phy-handle = <&phy3>;

	qca955x-serdes-fixup;

	gmac-config {
		device = <&gmac>;
		rxdv-delay = <3>;
		rxd-delay = <3>;
		rgmii-enabled = <1>;
		serdes-cal = <0xE>;
	};
};

&wmac {
	status = "okay";

	qca,no-eeprom;
};

&pcie0 {
	status = "okay";

	wifi@0,0 {
		compatible = "pci168c,0033";
		reg = <0x0000 0 0 0 0>;
		qca,no-eeprom;
		#gpio-cells = <2>;
		gpio-controller;
	};

	wifi@0,1 {
		compatible = "pci168c,0033";
		reg = <0x0000 0 0 0 0>;
		qca,no-eeprom;
		#gpio-cells = <2>;
		gpio-controller;
	};
};

&pcie1 {
	status = "okay";

	wifi@0,0 {
		compatible = "pci168c,0033";
		reg = <0x0000 0 0 0 0>;
		qca,no-eeprom;
		#gpio-cells = <2>;
		gpio-controller;
	};

	wifi@0,1 {
		compatible = "pci168c,0033";
		reg = <0x0000 0 0 0 0>;
		qca,no-eeprom;
		#gpio-cells = <2>;
		gpio-controller;
	};
};